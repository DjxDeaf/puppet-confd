[confd]
confdir = <%= @confdir %>
<% if @prefix -%>
prefix = <%= @prefix %>
<% end -%>
<% if @interval -%>
interval = <%= @interval %>
<% end -%>
<% if @debug != 'nil' -%>
debug = <%= @debug %>
<% end -%>
<% if @confdnoop != 'nil' -%>
noop = <%= @confdnoop %>
<% end -%>
<% if @quiet != 'nil' -%>
quiet = <%= @quiet %>
<% end -%>
<% if @srv_domain -%>
srv_domain = <%= @srv_domain %>
<% end -%>
<% if @verbose != 'nil' -%>
verbose = <%= @verbose %>
<% end -%>

<% if @consul != 'nil' -%>
consul = <%= @consul %>
<% end -%>
<% if @consul_addr -%>
prefix = <%= @consul_addr %>
<% end -%>
<% if @etcd_nodes -%>
etcd_nodes = [
<% etcd_nodes.each do |node| -%>
  "<%= @node %>",
<% end -%>
]
<% end -%>
<% if @etcd_scheme -%>
etcd_scheme = <%= @etcd_scheme %>
<% end -%>

<% if @client_cert -%>
client_cert = <%= @client_cert_path %>
<% end -%>
<% if @client_key -%>
client_cert = <%= @client_key_path %>
<% end -%>
